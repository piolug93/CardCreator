<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.FlowPane?>

<fx:root hgap="5" type="javafx.scene.layout.FlowPane" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
    <TextField fx:id="keyField" text="Key" editable="false"/>
    <TextField text="Value" fx:id="valueField" />
    <Button text="Remove" onAction="#removeButtonAction" id="buttonRemove" stylesheets="/Stylesheets/MainFrame.css"/>
    <padding>
        <Insets bottom="0" left="0" right="5" top="5" />
    </padding>
</fx:root>
